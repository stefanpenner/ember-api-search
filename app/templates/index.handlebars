    {{view Ember.TextField valueBinding=query}}
    <ul>
      {{#each item in model.classes}}
        <li>{{item.name}}</li>
      {{/each}}
      {{#each item in model.files}}
        <li>{{item.name}}</li>
      {{/each}}
      {{#each item in model.modules}}
        <li>{{item.name}}</li>
      {{/each}}
      {{#each item in model.classItems}}
        <li>{{item.name}}{{item.file}}:{{item.line}}{{item.type}}</li>
      {{/each}}
    </ul>
    </ul>
